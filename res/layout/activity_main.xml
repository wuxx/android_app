<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    
    android:orientation="vertical"
        
    tools:context=".MainActivity" >
	<LinearLayout
	        android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:orientation="horizontal"
	    >
	    
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="一"
	        android:gravity="center"
	        android:textSize="20sp"
	        android:id="@+id/textview_string_index"
	        android:layout_weight="1"           
	         />

	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="弦"      
	        android:gravity="center"
	        android:textSize="20sp" 	        
	        android:layout_weight="1"               
	         />    
  
	    
	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="5"
	        android:gravity="center"
	        android:textSize="20sp"       
	        android:id="@+id/textview_freq_index"
	        android:layout_weight="1"               
	         />  
	           
		<TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="品"
	        android:gravity="center"
	        android:textSize="20sp"        	        
	        android:layout_weight="1"               
	         />  	         
	</LinearLayout>

	<TextView
	    android:id="@+id/textview_ret"
	    android:layout_width="280dp"
	    android:layout_height="wrap_content"
	    android:gravity="center"
	    android:text=""
	    android:textSize="40sp" />
         	
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:paddingBottom="10dp"
	    android:paddingTop="10dp" >

		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextC"
	     	android:id="@+id/btnC"	     	 
			android:onClick="clickHandler"
			android:layout_weight="1"     				
	     ></Button>
		
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextC_s"
	     	android:id="@+id/btnC_s"	     	
	     	android:onClick="clickHandler"
	     	android:layout_weight="1"     	 	     	           
	     ></Button>
			
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextD"
	     	android:id="@+id/btnD"	     	
	     	android:onClick="clickHandler" 
	     	android:layout_weight="1"
	     ></Button>
					
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextD_s"
	     	android:id="@+id/btnD_s"	     	
	     	android:onClick="clickHandler" 
	     	android:layout_weight="1"
	     ></Button>
    
    </LinearLayout>
		
	<LinearLayout
	        android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:orientation="horizontal"
	    >
		<Button
	   		android:layout_width="fill_parent"
	    	android:layout_height ="fill_parent"
	    	android:text="@string/btnTextE"
	    	android:id="@+id/btnE"
	    	android:layout_weight="1"  
	    	android:onClick="clickHandler"
	    ></Button>
		  
		
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextF"
	     	android:id="@+id/btnF"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
	     ></Button>
			             
	    <Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextF_s"
	     	android:id="@+id/btnF_s"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
	     ></Button> 

		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextG"
	     	android:id="@+id/btnG"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
	     ></Button>
		    
	 </LinearLayout>
			
	<LinearLayout
	        android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:orientation="horizontal"
	    >
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextG_s"
	     	android:id="@+id/btnG_s"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
	     ></Button>	   
	     
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextA"
	     	android:id="@+id/btnA"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
	     ></Button>	   
	               
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextA_s"
	     	android:id="@+id/btnA_s"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
	     ></Button>
			   		     
		<Button
	    	android:layout_width="fill_parent"
	     	android:layout_height ="fill_parent"
	     	android:text="@string/btnTextB"
	     	android:id="@+id/btnB"
	     	android:layout_weight="1"  
	     	android:onClick="clickHandler"
		></Button>	             
			    		
	</LinearLayout>
	
	<TextView
	    android:id="@+id/textview_timer"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center"
	    android:text=""
	    android:textSize="40sp" 
	     />
				             
   	</LinearLayout>
